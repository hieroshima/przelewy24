{
    "name": "hieroshima/przelewy24",
    "type": "library",
    "description": "PHP library for handling payments from przelewy24.pl",
    "keywords": ["hieroshima", "przelewy24", "payments"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Kowalski"
        }
    ],
    "require": {
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": {
            "Przelewy24\\": "src/Przelewy24/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Przelewy24\\": "tests/"
        }
    }
}